{
  "compilerOptions": {
    "lib": ["esnext"],
    "module": "esnext",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strict": true,
    "types": ["node"],
    "target": "esnext",
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "newLine": "lf",
    "preserveConstEnums": true,
    "outDir": "dist",
    "stripInternal": true
  },
  "include": ["src/*.ts"]
}
